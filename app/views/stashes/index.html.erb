<h1>Stash List</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Created at</th>
      <th>Updated at</th>
    </tr>
  </thead>
  <tbody>
    <% @stashes.each do |stash| %>
      <tr>
        <td><%= stash.id %></td>
        <td><%= stash.created_at %></td>
        <td><%= stash.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
